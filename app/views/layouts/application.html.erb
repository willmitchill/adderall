<!DOCTYPE html>
<html>
<head>
  <title>Adderall</title>
  <script src="http://code.jquery.com/jquery-latest.js"></script>
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>


  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>


  <%= javascript_include_tag 'jquery.fancybox', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'jquery.fancybox-buttons', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'jquery.fancybox-media', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'jquery.fancybox-thumbs', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'jquery.fancybox.pack', 'data-turbolinks-track' => true %>

  <%= stylesheet_link_tag    'jquery.fancybox-buttons', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'jquery.fancybox-thumbs', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'jquery.fancybox', media: 'all', 'data-turbolinks-track' => true %>


</head>
<body>
  <%= render 'layouts/footer' %>
  <div id="body">
    <%= yield %>
  </div>

</body>
</html>
